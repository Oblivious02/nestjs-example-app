{
  "name": "nestjs-prisma-client-starter",
  "version": "0.0.1",
  "description": "NestJS Prisma Client Starter Project",
  "author": "Ismael Ramos",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "Prisma",
    "Prisma Client",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ismaestro/nestjs-example-app.git"
  },
  "engines": {
    "node": "16.15.0"
  },
  "bugs": {
    "url": "https://github.com/Ismaestro/nestjs-example-app/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "docker:build:server": "docker build -t nestjs-heroes-app . && docker run -d -t -p 3000:3000 nestjs-heroes-app",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "seed": "prisma db seed",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:push": "prisma db push",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prebuild": "rimraf dist",
    "build": "nest build"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "2.2.1",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "8.4.6",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "8.4.6",
    "@nestjs/graphql": "9.1.2",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.6",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/terminus": "9.1.1",
    "apollo-server-express": "3.8.1",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "graphql": "15.8.0",
    "graphql-subscriptions": "2.0.0",
    "helmet": "5.1.0",
    "joi": "17.6.0",
    "nestjs-i18n": "^10.2.3",
    "nestjs-prisma": "0.15.1",
    "node-mailjet": "5.1.1",
    "nodemailer": "6.7.5",
    "passport": "0.6.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "swagger-ui-express": "4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.6",
    "@nestjs/testing": "8.4.6",
    "@prisma/client": "^4.4.0",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.0",
    "@types/node": "17.0.39",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "chance": "1.1.8",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "1.9.0",
    "eslint-plugin-rxjs-angular": "2.0.0",
    "eslint-plugin-security": "1.5.0",
    "eslint-plugin-sonarjs": "0.16.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "prettier-eslint": "15.0.1",
    "pretty-quick": "3.1.3",
    "prisma": "^4.4.0",
    "prisma-dbml-generator": "0.9.1",
    "supertest": "6.2.3",
    "ts-jest": "28.0.4",
    "ts-loader": "9.3.0",
    "ts-node": "10.8.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
